    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-auth-library": "^9.5.0",
    "googleapis": "^131.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
