{
  "name": "informe5backend",
  "version": "1.0.0",
  "main": "./dist/app.js",
  "scripts": {
    "dev": "prisma generate && ts-node-dev  --respawn  --poll --transpile-only --exit-child --no-notify --ignore-watch node_modules  app.ts",
